using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Magic.Common.Robots
{
	/// <summary>
	/// This is the base interface all robotic vehicles derive from
	/// </summary>
	public interface IRobot
	{
		double Width{get;}
		double Length{get;}
		double Height{get;}
		IRobotShape Shape { get; }
	}
	public enum IRobotShape
	{
		Circular,
		Rectangular
	}
}
